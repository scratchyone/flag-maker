    "next": "^12.0.7",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.1.0",
    "raf": "^3.4.1",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-scroll-to-component": "^1.0.2",
